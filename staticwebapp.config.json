{
  "navigationFallback": {
    "rewrite": "/"
  },
  "routes": [
    {
        "route": "/api/*",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ],
        "allowedRoles": [
          "authenticated"
        ]
    },
    {
      "route": "/*",
      "rewrite": "/"
    }
  ]
}